<core:FragmentDefinition xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns="sap.m">
    <Dialog id="deleteDialog" contentWidth="650px" icon="https://www.signiwis.com/images/logo.jpg">
        <List items="{empData>}"  mode="MultiSelect" id="idDeleteList">
            <items>
                <ObjectListItem title="{empData>name}" ></ObjectListItem>
            </items>
        </List>
        <footer>
            <Toolbar id="_IDGenToolbar1" >
                <ToolbarSpacer id="_IDGenToolbarSpacer1" ></ToolbarSpacer>
                <Button id="_IDGenButton1" text="DELETE" type="Reject"  press="onItemPressDelete" icon="sap-icon://delete"></Button>
                <Button id="_IDGenButton2" text="Cancel" type="Back" icon="sap-icon://cancel" press="deleteCancel"></Button>
            </Toolbar>
        </footer>

    </Dialog>
</core:FragmentDefinition>